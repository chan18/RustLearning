{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build",
			"options": {
                "cwd": "${workspaceFolder}/debugging"  // Ensure this is pointing to 'debugging'
            }
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo clean",
			"options": {
                "cwd": "${workspaceFolder}/debugging"  // Ensure this is pointing to 'debugging'
            }
		}
	]
}